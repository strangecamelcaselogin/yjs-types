{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2015",
    "lib": [
      "es6",
      "dom"
    ],
    "declaration": true,
    "removeComments": false,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src",
  ]
}
